{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <p>Identifiant: {{ user.username }}</p>

    <p>Nom: {{ user.Nom }}</p>

    <p>Prenom: {{ user.Prenom }}</p>

    <p>avatar :
        <img src="{{ asset(upload_path ~ app.user.path) }}" height="60px" width="60px"/>
    </p>
    <p>Signature :
        <img src="{{ asset(upload_path_signatures ~ app.user.pathSignature) }}" height="60px" />
    </p>
</div>